<env>
<!-- actuactor : 'pd', 'torque', 'muscle', 'mass', 'muspd'  -->
<skeleton defaultKp="250" defaultKv="10" actuactor="pd" damping="0.2">data/skel.xml</skeleton>
<ground>data/ground.xml</ground>

<!-- motion set  -->
<!-- single clip -->
<bvh symmetry="false" heightCalibration="true" firstT="true">data/motion/backflip.bvh</bvh>
<!-- Advanced Option -->
<eoeheight>0.0</eoeheight>
<useRootXYZ>false</useRootXYZ>

<!-- Making BVH Cyclic -->
<cyclicbvh>true</cyclicbvh>

<!-- Controller Parameter -->
<residual>true</residual>

<!-- Simulation Setting  -->
<simHz>480</simHz>
<controlHz>30</controlHz>

<!-- Muscle Control  -->
<!-- <muscle>../data/muscle284.xml</muscle> -->
<inferencepersim>1</inferencepersim> 

<!-- Advanced Option -->
<heightCalibration strict="false">true</heightCalibration>

<enforceSymmetry>false</enforceSymmetry>
<actionScale>0.04</actionScale>

<!-- -1 means not using -->
<!-- <timeWarping>0.001</timeWarping>  -->
<stanceLearning>false</stanceLearning>
<metabolicReward>true</metabolicReward>
<MetabolicWeight>4.0</MetabolicWeight>

<!-- Advanced Option (Muscle) -->
<meshLbsWeight>false</meshLbsWeight>
<useVelocityForce>false</useVelocityForce>
<useJointState>false</useJointState>
<learningStd>false</learningStd>
<torqueClipping>false</torqueClipping>
<includeJtPinSPD>false</includeJtPinSPD>
<useNormalizedParamState>false</useNormalizedParamState>
<useNewState>true</useNewState>

<!-- motion information : phase or target-->
<motionInfoState wsize="1">target</motionInfoState>
<constantMotionInfoState>true</constantMotionInfoState>

<!-- eoe : 'time', 'tuple', 'amp' -->
<eoeType>tuple</eoeType>
<rewardType>scadiver</rewardType>

<rsiTuple>299</rsiTuple>
<Horizon>300</Horizon>

<HighResolutionRollout>false</HighResolutionRollout>
</env>
